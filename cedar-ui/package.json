{
  "name": "cedar-ui",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run compile && npm run bundle && npm run combine",
    "compile": "babel src/* src/**/* -d _tmp --blacklist es6.modules -e 0",
    "bundle": "rollup _tmp/src/cedar-ui.js -o dist/cedar.js",
    "combine": "uglifyjs node_modules/document-register-element/build/document-register-element.js dist/bundle.js -o dist/cedar-ui.min.js --source-map",
    "server": "http-server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.21",
    "http-server": "^0.8.0",
    "rollup": "^0.12.1",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {
    "document-register-element": "^0.4.5"
  }
}
